{
  "name": "zenn-contents",
  "image": "mcr.microsoft.com/vscode/devcontainers/javascript-node:0-16-bullseye",
  "extensions": ["GitHub.vscode-pull-request-github"],
  "portsAttributes": {
    "8000": {
      "label": "Zenn Preview",
      "onAutoForward": "openBrowser"
    }
  },
  "features": {
    "github-cli": "latest"
  },
  "postStartCommand": "npm install",
  "remoteUser": "node"
}
